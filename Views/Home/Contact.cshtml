@{
    ViewBag.Title = "Contact";
}

<main aria-labelledby="title">
    <h2 id="title">@ViewBag.Title.</h2>

    <address>
        <strong>Address:</strong><br />
        @ViewBag.Address<br />

        <strong>Phone:</strong><br />
        @ViewBag.Phone<br />

        <strong>Email:</strong><br />
        <a href="mailto:@ViewBag.Email">@ViewBag.Email</a><br />
    </address>
</main>